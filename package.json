{
  "name": "talks",
  "type": "module",
  "private": true,
  "scripts": {
    "setupfolder": "node generateDirs.js",
    "build": "for folderName in $(basename -a slides/*/); do slidev build --base /talks/${folderName} slides/${folderName}/${folderName}.md  --out ../../dist/${folderName} --per-slide; done",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@iconify/json": "^2.2.179",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "playwright-chromium": "^1.41.2",
    "pnpm": "^8.15.1",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "slidev-theme-antfu-vite-conf-2023": "^0.1.9",
    "typescript": "^5.3.3",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.19",
    "@slidev/cli": "^0.48.9"
  }
}
